{
  "name": "@ctvoicer/ctstyle",
  "version": "1.2.1",
  "description": "Uma customização do framework CSS Bulma para os produtos CTVoicer.",
  "author": "CTVoicer <webmaster@ctvoicer.com.br>",
  "license": "UNLICENSED",
  "keywords": [
    "css",
    "sass",
    "flexbox",
    "responsive",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "git@ssh.dev.azure.com:v3/CTVoicer/CTStyle/ctstyle"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "main": "./dist/ctstyle.css",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "dev": "cross-env NODE_ENV=development webpack --mode development --watch",
    "link": "[ -L ./docs/assets ] || ln -s ../dist ./docs/assets",
    "serve": "cross-env NODE_ENV=production npm run build && npm run link && http-server ./docs -c-1 -p 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "bulma": "^0.7.5",
    "clean-webpack-plugin": "^2.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "http-server": "^0.11.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^7.1.0",
    "standard-version": "^5.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  }
}
